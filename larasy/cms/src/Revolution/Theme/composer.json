{
    "name": "revolution/theme",
    "description": "theme manage all functionalities to handle larasy themes",
    "keywords": ["theme", "larasy", "skin"],
    "homepage": "https://larasy.github.io/theme",
    "type": "library",
    "license": "Creative Commons",
    "authors": [
        {
            "name": "Nahid Bin Azhar",
            "email": "nahid.dns@gmail.com"
        }
    ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/contracts": "5.0.* || 5.1.* || 5.2.* || 5.3.*",
    "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.*"
  },
  "require-dev": {
    "graham-campbell/testbench": "^3.1",
    "phpunit/phpunit": "^4.8 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Theme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Revolution\\Theme\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
